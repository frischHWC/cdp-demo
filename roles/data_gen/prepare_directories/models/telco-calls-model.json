    {"OZONE_KEY":  "account_id_calling"},
    {"KUDU_PRIMARY_KEYS":  "account_id_calling,account_id_receiving"},
    {"KUDU_HASH_KEYS":  "account_id_receiving"},
    {"KUDU_RANGE_KEYS":  "account_id_calling,account_id_receiving"}
